{
	"name": "leek-wars-client",
	"version": "2.11.1",
	"scripts": {
		"start": "vue-cli-service serve --mode dev",
		"serve": "vue-cli-service serve --mode dev",
		"beta": "vue-cli-service serve --mode beta",
		"local": "vue-cli-service serve --mode local",
		"build": "VUE_MODE=build vue-cli-service build",
		"build-beta": "VUE_MODE=build NODE_ENV=production vue-cli-service build --mode beta",
		"lint": "vue-cli-service lint",
		"stylelint": "stylelint src/**/*.{vue,css,scss}",
		"test": "bundlesize"
	},
	"dependencies": {
		"codemirror": "^5.52.0",
		"dotenv": "^8.2.0",
		"es6-promise": "^4.2.8",
		"js-beautify": "^1.10.3",
		"katex": "^0.10.2",
		"markdown-it": "^8.4.2",
		"register-service-worker": "^1.7.0",
		"swiper": "^5.4.5",
		"vue": "^2.6.11",
		"vue-awesome-swiper": "^3.1.3",
		"vue-chartist": "^2.2.1",
		"vue-i18n": "^8.15.5",
		"vue-property-decorator": "^7.3.0",
		"vue-router": "^3.1.6",
		"vuetify": "^2.2.17",
		"vuex": "^3.1.3",
		"yaml-loader": "^0.5.0"
	},
	"devDependencies": {
		"@kazupon/vue-i18n-loader": "^0.3.0",
		"@types/codemirror": "0.0.60",
		"@types/google.analytics": "0.0.39",
		"@types/node": "^12.12.30",
		"@typescript-eslint/eslint-plugin": "^2.23.0",
		"@typescript-eslint/parser": "^2.23.0",
		"@typescript-eslint/typescript-estree": "^2.23.0",
		"@vue/cli-plugin-eslint": "^4.2.3",
		"@vue/cli-plugin-pwa": "^3.12.1",
		"@vue/cli-plugin-typescript": "^3.12.1",
		"@vue/cli-service": "^4.2.3",
		"@vue/eslint-config-airbnb": "^5.0.2",
		"@vue/eslint-config-prettier": "^6.0.0",
		"@vue/eslint-config-standard": "^5.1.2",
		"@vue/eslint-config-typescript": "^5.0.2",
		"babel-plugin-transform-imports": "^1.4.1",
		"bundlesize": "^0.17.2",
		"eslint": "^6.8.0",
		"eslint-plugin-vue": "^6.2.2",
		"json-loader": "^0.5.7",
		"prettier": "1.14.2",
		"request": "^2.88.2",
		"request-promise": "^4.2.5",
		"sass": "^1.26.3",
		"sass-loader": "^8.0.2",
		"stylelint": "^9.10.1",
		"stylelint-config-prettier": "^4.0.0",
		"stylelint-config-recommended": "^2.2.0",
		"stylelint-processor-html": "^1.0.0",
		"stylelint-scss": "^3.14.2",
		"stylus": "^0.54.7",
		"stylus-loader": "^3.0.2",
		"terser-webpack-plugin": "^2.3.5",
		"tslint": "^5.20.1",
		"tslint-config-prettier": "^1.18.0",
		"tslint-config-standard": "^8.0.1",
		"typescript": "^3.8.3",
		"typescript-estree": "^18.1.0",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"vue-cli-plugin-vuetify": "^0.1.6",
		"vue-template-compiler": "^2.6.11",
		"vuetify-loader": "^1.4.3",
		"webpack-bundle-analyzer": "^3.6.1"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"@vue/typescript"
		],
		"parserOptions": {
			"parser": "typescript-estree"
		},
		"rules": {
			"vue/attribute-hyphenation": "error",
			"vue/html-end-tags": "error",
			"vue/mustache-interpolation-spacing": "error",
			"vue/name-property-casing": "error",
			"vue/no-multi-spaces": "error",
			"vue/require-default-prop": "error",
			"vue/require-prop-types": "error",
			"vue/v-bind-style": "error",
			"vue/v-on-style": "error",
			"vue/attributes-order": "error",
			"vue/html-quotes": "error",
			"vue/html-indent": [
				"error",
				"tab",
				{
					"attribute": 1,
					"closeBracket": 0,
					"alignAttributesVertically": true
				}
			],
			"vue/html-self-closing": [
				"error",
				{
					"html": {
						"normal": "never",
						"component": "always"
					},
					"svg": "always"
				}
			],
			"vue/this-in-template": [
				"error",
				"never"
			],
			"vue/html-closing-bracket-spacing": [
				"error",
				{
					"startTag": "never",
					"endTag": "never",
					"selfClosingTag": "always"
				}
			],
			"vue/no-confusing-v-for-v-if": "error",
			"vue/no-use-v-if-with-v-for": "off"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	],
	"license": "GPL-3.0",
	"author": "Pilow",
	"description": "Leek Wars game client website",
	"repository": {
		"type": "git",
		"url": "git+https://5pilow@github.com/leek-wars/leek-wars-client.git"
	},
	"bugs": {
		"url": "https://github.com/leek-wars/leek-wars-client/issues"
	},
	"homepage": "https://github.com/leek-wars/leek-wars-client#readme",
	"bundlesize": [
		{
			"path": "./dist/js/*.js",
			"maxSize": "250kB"
		}
	]
}
